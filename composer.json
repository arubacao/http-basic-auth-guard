{
    "name": "arubacao/http-basic-auth-guard",
    "description": "HTTP Basic Auth Guard for Lumen 5.2",
    "keywords": [
        "lumen",
        "lumen 5.2",
        "http basic auth guard lumen",
        "http basic auth guard lumen 5.2",
        "http basic auth guard",
        "http basic auth",
        "http basic authentication"
    ],
    "homepage": "https://github.com/arubacao/http-basic-auth-guard",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Lass",
            "email": "arubacao@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/arubacao/http-basic-auth-guard/issues",
        "source": "https://github.com/arubacao/http-basic-auth-guard"
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/database": "^5.2",
        "illuminate/support": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.1",
        "mockery/mockery": "dev-master",
        "graham-campbell/testbench": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Arubacao\\BasicAuth\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/models/User.php",
            "tests/AbstractTestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
